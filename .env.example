# Image Generation API Configuration
# Set IMAGE_API_PROVIDER to "openai" or "openrouter"
IMAGE_API_PROVIDER=openai

# OpenAI API Key (required if IMAGE_API_PROVIDER=openai)
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# OpenRouter API Key (required if IMAGE_API_PROVIDER=openrouter)
# Get your API key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Telegram Bot Configuration
# Get your bot token from: https://t.me/BotFather
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Telegram Chat ID where memes will be sent
# You can find your chat ID by messaging @userinfobot on Telegram
TELEGRAM_CHAT_ID=your_telegram_chat_id_here
